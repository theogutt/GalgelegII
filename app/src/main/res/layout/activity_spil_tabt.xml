<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SpilTabtActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="DU HAR TABT"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/textView_ordetVar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView_ordetVar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="320dp"
        android:layout_marginEnd="29dp"
        android:text="Ordet var:       "
        android:textSize="36sp"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_spil_igen2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="21dp"
        android:layout_marginTop="12dp"
        android:text="Spil igen"
        app:layout_constraintStart_toStartOf="@+id/button_se_highscore2"
        app:layout_constraintTop_toBottomOf="@+id/button_se_highscore2" />

    <Button
        android:id="@+id/button_se_highscore2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="1dp"
        android:text="se highscores"
        app:layout_constraintEnd_toEndOf="@+id/textView_ordetVar"
        app:layout_constraintTop_toBottomOf="@+id/textView_ordetVar" />
</androidx.constraintlayout.widget.ConstraintLayout>