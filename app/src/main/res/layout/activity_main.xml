<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"><![CDATA[


app:layout_constraintBottom_toBottomOf="parent"
app:layout_constraintEnd_toEndOf="parent"
app:layout_constraintStart_toStartOf="parent" />

app:layout_constraintBottom_toTopOf="@+id/button_highscore"
app:layout_constraintStart_toStartOf="@+id/button_highscore" />



    ]]>

    <Button
        android:id="@+id/button_highscore"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Highscore"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_spil"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:layout_marginBottom="35dp"
        android:text="Spil"
        app:layout_constraintBottom_toTopOf="@+id/button_highscore"
        app:layout_constraintStart_toStartOf="@+id/button_highscore" />

    <Button
        android:id="@+id/button_hjaelp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="6dp"
        android:layout_marginTop="44dp"
        android:text="Hjælp"
        app:layout_constraintStart_toStartOf="@+id/button_highscore"
        app:layout_constraintTop_toBottomOf="@+id/button_highscore" />

    <Button
        android:id="@+id/button_svaerhedsgrad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:text="SVÆRHEDSGRAD"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_hjaelp" /><![CDATA[

    app:layout_constraintStart_toStartOf="@+id/button_highscore"
    app:layout_constraintTop_toBottomOf="@+id/button_highscore" />
]]>
</androidx.constraintlayout.widget.ConstraintLayout>